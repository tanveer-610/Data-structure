#include<stdio.h>
int main()
{
    int i,p,n,a[100];
    printf("enter how many element in ur array:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("which position want to delete:");
    scanf("%d",&p);
    for(i=p-1;i<n-1;i++)
    {
        a[i]=a[i+1];
    }
    for(i=0;i<n-1;i++)
    {
        printf("%d\n",a[i]);
    }
}
