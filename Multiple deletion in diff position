#include<stdio.h>
int main()
{
    int a[20],e,el,pos,i,n;
    printf("How many elements in that array ? ");
    scanf("%d",&el);
    e=el;
    printf("Enter those elements : ");
        for(int i=0;i<e;i++)
       scanf("%d",&a[i]);
    printf("How many elements you want to delete? ");
    scanf("%d",&n);
    for(int i=0;i<n;i++,e--)
  {
    printf("Enter the position : ");
    scanf("%d",&pos);
      for(int i=pos-1;i<e-1;i++)
        a[i]=a[i+1];
    printf("Okay Done !!!!");
}
 printf("New array : ");
      for(int i=0;i<el-n;i++)
       printf("%d\n",a[i]);
}
